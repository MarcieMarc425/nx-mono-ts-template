{
  "name": "template",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Marc Tse",
    "url": "https://github.com/MarcieMarc425"
  },
  "description": "A production-grade, opinionated NX monorepo template for enterprise. Bundled with frontend NextJS, backend NestJS, libraries, and more.",
  "scripts": {
    "git:cleanup": "git fetch -p && for branch in `git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == \"[gone]\" {sub(\"refs/heads/\", \"\", $1); print $1}'`; do git branch -D $branch; done",
    "start": "nx serve",
    "start:all": "nx run-many --target=serve --all --parallel=3",
    "build": "nx build",
    "build:all": "nx run-many --target=build --all --parallel=3",
    "build:affected": "nx affected:build",
    "test": "nx test",
    "test:all": "nx run-many --target=test --all --parallel=3",
    "test:affected": "nx affected:test",
    "lint": "nx lint",
    "lint:all": "nx run-many --target=lint --all --parallel=3",
    "lint:affected": "nx affected:lint",
    "ci": "pnpm build:affected && pnpm lint:affected && pnpm test:affected",
    "commit": "cz",
    "prepare": "husky install",
    "version": "changeset version"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.5.9",
    "@anatine/zod-nestjs": "^1.7.4",
    "@anatine/zod-openapi": "^1.9.8",
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@next/bundle-analyzer": "^12.2.5",
    "@swc/helpers": "~0.3.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "core-js": "^3.6.5",
    "nestjs-pino": "^3.1.1",
    "next": "12.2.5",
    "next-compose-plugins": "^2.2.1",
    "openapi3-ts": "^3.0.2",
    "pino-http": "^8.2.0",
    "pino-pretty": "^9.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
